{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2020",
        "lib": ["es2018", "dom"],
        "paths": {
            "@ahmed757/*": [
                "dist/ahmed757/ngx-treant-js/ahmed757-ngx-treant-js",
                "dist/ahmed757/ngx-treant-js"
            ]
        },
        "allowJs": true
    },
    "angularCompilerOptions": {
        "fullTemplateTypeCheck": true,
        "strictInjectionParameters": true
    }
}
