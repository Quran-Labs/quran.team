{
  "name": "quran-team",
  "version": "0.1.7",
  "author": "Tarek Eldeeb",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-lib": "ng build @ahmed757/ngx-treant-js",
    "build": "npm run build-lib && ng build --base-href / --configuration production && git checkout -- docs/CNAME",
    "clean": "rm -rf ./dist & rm -rf node_modules & npm cache clean --force",
    "test": "ng test",
    "lint": "ng lint",
    "prettier-format": "prettier --config .prettierrc '**/*.{ts,js,html,css,json}' --write",
    "e2e": "ng e2e",
    "deploy": "gh-pages -d dist",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/AhmedAlatawi/ngx-treant-js.git"
  },
  "bugs": {
    "url": "https://github.com/Quran-Labs/quran.team/issues"
  },
  "homepage": "https://quran.team",
  "keywords": [
    "quran",
    "sanad",
    "database",
    "islam",
    "ejaza",
    "jquery",
    "visualization",
    "tree",
    "diagrams",
    "draws",
    "charts",
    "svg"
  ],
  "dependencies": {
    "@ahmed757/ngx-treant-js": "file:lib/ngx-treant-js",
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/localize": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@octokit/core": "^5.0.2",
    "bootstrap": "4.5.0",
    "hijri-js": "^1.0.25",
    "jquery": "3.5.1",
    "jquery-easing": "0.0.1",
    "ngx-bootstrap": "10.2",
    "ngx-scrollbar": "^10.0",
    "octokit-plugin-create-pull-request": "^5.1.1",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "treant-js": "file:lib/treant-js",
    "tslib": "^2.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-devkit/build-ng-packagr": "^0.1002.4",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^19.2.2",
    "@cypress/schematic": "^2.5.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.38",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "gh-pages": "^6.0.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.2.11",
    "ng-packagr": "^15.2.2",
    "prettier": "2.0.5",
    "semantic-release": "^19.0.3",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.3",
    "typescript": "~4.9.5",
    "cypress": "latest"
  }
}
