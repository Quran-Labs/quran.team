  },
  "bugs": {
    "url": "https://github.com/Quran-Labs/quran.team/issues"
  },
  "homepage": "https://quran.team",
  "keywords": [
    "quran",
    "sanad",
    "database",
    "islam",
    "ejaza",
    "jquery",
    "visualization",
    "tree",
    "diagrams",
    "draws",
    "charts",
    "svg"
