    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "bootstrap": "4.5.0",
    "jquery": "3.5.1",
    "jquery-easing": "0.0.1",
    "ngx-bootstrap": "^5.6.1",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "treant-js": "file:lib/treant-js",
    "tslib": "^2.6.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~11.2.19",
    "@angular/compiler-cli": "~11.2.14",
